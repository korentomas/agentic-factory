# ── AgentFactory Orchestrator Environment Variables ───────────────────────────
# Copy to .env and fill in. Never commit .env to version control.

# ── ClickUp ───────────────────────────────────────────────────────────────────
# Webhook secret — set when registering the ClickUp webhook
CLICKUP_WEBHOOK_SECRET=your_clickup_webhook_secret_here

# ClickUp personal API token (Settings → Apps → API Token)
CLICKUP_API_TOKEN=pk_your_clickup_api_token_here

# ClickUp Team ID (visible in ClickUp URL: app.clickup.com/TEAM_ID/...)
CLICKUP_TEAM_ID=your_team_id_here

# ── GitHub ────────────────────────────────────────────────────────────────────
# GitHub App credentials — used by workflows via actions/create-github-app-token.
# For the orchestrator, set these as env vars. For GitHub Actions, set as secrets.
# Create a GitHub App: https://github.com/settings/apps → New GitHub App
# Required permissions: Contents (R/W), Pull requests (R/W), Issues (R/W)
GITHUB_APP_ID=123456
GITHUB_APP_PRIVATE_KEY=

# Target repository in org/repo format
GITHUB_REPO=your-org/your-repo

# ── Slack ─────────────────────────────────────────────────────────────────────
# Incoming Webhook URL from api.slack.com/apps → Incoming Webhooks
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK

# Slack channel to post notifications to (without #)
SLACK_CHANNEL=dev-agents

# ── Orchestrator ──────────────────────────────────────────────────────────────
# Public URL of this orchestrator service (for GitHub Actions callbacks)
ORCHESTRATOR_URL=https://agent-factory-xxxx-uc.a.run.app

# Shared secret for validating GitHub Actions callback requests
# Generate with: python -c "import secrets; print(secrets.token_hex(32))"
CALLBACK_SECRET=your_callback_secret_here

# ── Optional: Database (for state persistence / deduplication) ────────────────
# Leave empty to use in-memory deduplication (resets on restart)
DATABASE_URL=

# ── AI Provider ────────────────────────────────────────────────────────────────
# For Anthropic direct: set ANTHROPIC_API_KEY in GitHub Actions secrets
# For OpenRouter: set ANTHROPIC_API_KEY to your OpenRouter key, then set
#   CLAUDE_SETTINGS secret to: {"env":{"ANTHROPIC_BASE_URL":"https://openrouter.ai/api/v1"}}
# See docs/providers.md for Bedrock, Vertex AI, and custom gateway setup.

# ── Logging ───────────────────────────────────────────────────────────────────
# debug | info | warning | error
LOG_LEVEL=info

# Set to "true" to pretty-print JSON logs (useful for local dev)
LOG_PRETTY=true
